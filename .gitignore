# Ignore node_modules for both backend and frontend
/node_modules/
/frontend/node_modules/
/backend/node_modules/
 
# Ignore build artifacts from frontend React app
/frontend/build/
/frontend/.env.local
/frontend/.env.development.local
/frontend/.env.test.local
/frontend/.env.production.local
/frontend/.cache/
/frontend/.vscode/
 
# Ignore build artifacts from backend Node.js app
/backend/dist/
/backend/.env.local
/backend/.env.development.local
/backend/.env.production.local
/backend/.env.test.local
/backend/.cache/
/backend/.vscode/
 
# Ignore logs
*.log
logs/
 
# Ignore OS generated files
.DS_Store
Thumbs.db
 
# Ignore npm package lock files
/frontend/package-lock.json
/backend/package-lock.json
 
# Ignore yarn lock files (if using Yarn in either backend or frontend)
frontend/yarn.lock
backend/yarn.lock
 
# Ignore IDE or editor folders
.vscode/
.idea/
 
# Ignore the environment config files for both frontend and backend
/frontend/.env
/backend/.env
 
# Ignore temporary files and caches
*.swp
*.swo
*.bak
*.tmp
*.orig
 
# Ignore coverage reports from testing
/coverage/
/frontend/coverage/
/backend/coverage/